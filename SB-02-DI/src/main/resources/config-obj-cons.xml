<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean id="myCar" class="com.ty.obj.cons.Car">
		<constructor-arg value="TATA"></constructor-arg>
		<constructor-arg value="1500000"></constructor-arg>
		<constructor-arg ref="myEngine"></constructor-arg>
	</bean>

	<bean id="myEngine" class="com.ty.obj.cons.Engine">
		<constructor-arg value="560"></constructor-arg>
	</bean>

</beans>